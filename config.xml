    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-1242@3x~iphone6s-landscape_2208x1242.png" height="1242" width="2208"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-1242@3x~iphone6s-portrait_1242x2208.png" height="2208" width="1242"/>    
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-568h@2x~iphone_640x1136.png" height="1136" width="640"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-750@2x~iphone6-landscape_1334x750.png" height="750" width="1334"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-750@2x~iphone6-portrait_750x1334.png" height="1334" width="750"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Landscape@2x~ipad_2048x1496.png" height="1496" width="2048"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Landscape@2x~ipad_2048x1536.png" height="1536" width="2048"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Landscape~ipad_1024x748.png" height="748" width="1024"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Landscape~ipad_1024x768.png" height="768" width="1024"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Portrait@2x~ipad_1536x2008.png" height="2008" width="1536"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Portrait@2x~ipad_1536x2048.png" height="2048" width="1536"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default-Portrait~ipad_768x1024.png" height="1024" width="768"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default.png" height="1004" width="768"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default@2x~iphone_640x960.png" height="960" width="640"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default~ipad.png" height="2008" width="1536"/>
    <splash platform="ios" src="www/img/iOS/Resources/splash/Default~iphone.png" height="480" width="320"/>

    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="darkcontent" />
    
    <gap:plugin name="org.apache.cordova.statusbar" version="0.1.4" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />

    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="org.apache.cordova.globalization" />
    <gap:plugin name="org.apache.cordova.file" spec="1.3.3" source="pgb" />
    <gap:plugin name="org.apache.cordova.file-transfer"  spec="0.5.0" source="pgb" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.1" />
    <plugin name="cordova-plugin-file" spec="~3.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.3.0" />
</widget>
